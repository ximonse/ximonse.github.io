        <rdf:value>&lt;div data-citation-items=&quot;%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22itemData%22%3A%7B%22id%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%2C%22type%22%3A%22book%22%2C%22abstract%22%3A%22DiVA%20portal%20is%20a%20finding%20tool%20for%20research%20publications%20and%20student%20theses%20written%20at%20the%20following%2050%20universities%20and%20research%20institutions.%22%2C%22language%22%3A%22swe%22%2C%22source%22%3A%22www.diva-portal.org%22%2C%22title%22%3A%22Matematikundervisning%20i%20f%C3%B6rskoleklass%20%3A%20att%20uppt%C3%A4cka%20matematiksv%C3%A5righeter%20och%20fr%C3%A4mja%20elevers%20matematikutveckling%22%2C%22title-short%22%3A%22Matematikundervisning%20i%20f%C3%B6rskoleklass%22%2C%22URL%22%3A%22https%3A%2F%2Furn.kb.se%2Fresolve%3Furn%3Durn%3Anbn%3Ase%3Amdh%3Adiva-58938%22%2C%22author%22%3A%5B%7B%22family%22%3A%22Karlsson%20Holst%22%2C%22given%22%3A%22Helene%22%7D%2C%7B%22family%22%3A%22Nilsson%22%2C%22given%22%3A%22%C3%85sa%22%7D%5D%2C%22accessed%22%3A%7B%22date-parts%22%3A%5B%5B%222023%22%2C6%2C1%5D%5D%7D%2C%22issued%22%3A%7B%22date-parts%22%3A%5B%5B%222022%22%5D%5D%7D%7D%7D%5D&quot; data-schema-version=&quot;9&quot;&gt;&lt;p&gt;&lt;span class=&quot;highlight&quot; data-annotation=&quot;%7B%22attachmentURI%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FPI6P72HR%22%2C%22annotationKey%22%3A%227BQSIRU7%22%2C%22color%22%3A%22%23ffd400%22%2C%22pageLabel%22%3A%222%22%2C%22position%22%3A%7B%22pageIndex%22%3A1%2C%22rects%22%3A%5B%5B71.053%2C486.481%2C507.934%2C497.021%5D%5D%7D%2C%22citationItem%22%3A%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%222%22%7D%7D&quot;&gt;&lt;span style=&quot;background-color: #ffd40080&quot;&gt;“Tidig matematisk förståelse har stor betydelse för framtida utbildningsresultat i matematik.”&lt;/span&gt;&lt;/span&gt; &lt;span class=&quot;citation&quot; data-citation=&quot;%7B%22citationItems%22%3A%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%222%22%7D%5D%2C%22properties%22%3A%7B%7D%7D&quot;&gt;(&lt;span class=&quot;citation-item&quot;&gt;Karlsson Holst och Nilsson, 2022, p. 2&lt;/span&gt;)&lt;/span&gt; Kommentar&lt;/p&gt;
&lt;p&gt;&lt;span class=&quot;highlight&quot; data-annotation=&quot;%7B%22attachmentURI%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FPI6P72HR%22%2C%22annotationKey%22%3A%22PIHZ8C2S%22%2C%22color%22%3A%22%23ff6666%22%2C%22pageLabel%22%3A%222%22%2C%22position%22%3A%7B%22pageIndex%22%3A1%2C%22rects%22%3A%5B%5B319.28%2C375.361%2C505.538%2C385.901%5D%2C%5B71.053%2C359.521%2C461.976%2C370.06%5D%2C%5B71.053%2C343.681%2C497.856%2C354.221%5D%2C%5B71.053%2C327.841%2C339.002%2C338.38%5D%5D%7D%2C%22citationItem%22%3A%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%222%22%7D%7D&quot;&gt;&lt;span style=&quot;background-color: #ff666680&quot;&gt;“Utifrån sociokulturella och relationella perspektiv med Bronfenbrenners ekologiska systemteori som utgångspunkt lyftes förskoleklasslärarnas och speciallärarnas/-pedagogernas arbete, elevernas utveckling och samspel och kommunikationen med omgivningen fram.”&lt;/span&gt;&lt;/span&gt; &lt;span class=&quot;citation&quot; data-citation=&quot;%7B%22citationItems%22%3A%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%222%22%7D%5D%2C%22properties%22%3A%7B%7D%7D&quot;&gt;(&lt;span class=&quot;citation-item&quot;&gt;Karlsson Holst och Nilsson, 2022, p. 2&lt;/span&gt;)&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;span class=&quot;highlight&quot; data-annotation=&quot;%7B%22attachmentURI%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FPI6P72HR%22%2C%22annotationKey%22%3A%22U6D2ZTUX%22%2C%22color%22%3A%22%235fb236%22%2C%22pageLabel%22%3A%222%22%2C%22position%22%3A%7B%22pageIndex%22%3A1%2C%22rects%22%3A%5B%5B385.608%2C232.561%2C519.91%2C243.1%5D%2C%5B71.053%2C216.721%2C520.83%2C227.26%5D%2C%5B71.053%2C200.881%2C444.916%2C211.42%5D%5D%7D%2C%22citationItem%22%3A%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%222%22%7D%7D&quot;&gt;&lt;span style=&quot;background-color: #5fb23680&quot;&gt;“Slutsatsen från denna studie är att förskoleklasslärarna och speciallärarna/-pedagogerna har olika material och metoder för att upptäcka och stödja elever som är i risk att utveckla matematiksvårigheter.”&lt;/span&gt;&lt;/span&gt; &lt;span class=&quot;citation&quot; data-citation=&quot;%7B%22citationItems%22%3A%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%222%22%7D%5D%2C%22properties%22%3A%7B%7D%7D&quot;&gt;(&lt;span class=&quot;citation-item&quot;&gt;Karlsson Holst och Nilsson, 2022, p. 2&lt;/span&gt;)&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;span class=&quot;highlight&quot; data-annotation=&quot;%7B%22attachmentURI%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FPI6P72HR%22%2C%22annotationKey%22%3A%22QAID3F6M%22%2C%22color%22%3A%22%232ea8e5%22%2C%22pageLabel%22%3A%222%22%2C%22position%22%3A%7B%22pageIndex%22%3A1%2C%22rects%22%3A%5B%5B71.053%2C121.441%2C480.243%2C131.98%5D%2C%5B71.053%2C105.601%2C329.305%2C116.141%5D%5D%7D%2C%22citationItem%22%3A%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%222%22%7D%7D&quot;&gt;&lt;span style=&quot;background-color: #2ea8e580&quot;&gt;“Nyckelord: främja och förebygga, förskoleklass, kartläggning, matematiksvårigheter, specialpedagogiska insatser, tidiga insatser, upptäcka.”&lt;/span&gt;&lt;/span&gt; &lt;span class=&quot;citation&quot; data-citation=&quot;%7B%22citationItems%22%3A%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%222%22%7D%5D%2C%22properties%22%3A%7B%7D%7D&quot;&gt;(&lt;span class=&quot;citation-item&quot;&gt;Karlsson Holst och Nilsson, 2022, p. 2&lt;/span&gt;)&lt;/span&gt; Kommentar&lt;/p&gt;
&lt;p&gt;&lt;span class=&quot;highlight&quot; data-annotation=&quot;%7B%22attachmentURI%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FPI6P72HR%22%2C%22annotationKey%22%3A%22F2G7N2RY%22%2C%22color%22%3A%22%23a28ae5%22%2C%22pageLabel%22%3A%225%22%2C%22position%22%3A%7B%22pageIndex%22%3A4%2C%22rects%22%3A%5B%5B71.053%2C717.601%2C520.242%2C728.14%5D%2C%5B71.053%2C701.761%2C245.03%2C712.3%5D%5D%7D%2C%22citationItem%22%3A%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%225%22%7D%7D&quot;&gt;&lt;span style=&quot;background-color: #a28ae580&quot;&gt;“Alla som arbetar i skolan ska uppmärksamma och stödja elever i behov av extra anpassningar och särskilt stöd (Skolverket, 2018).”&lt;/span&gt;&lt;/span&gt; &lt;span class=&quot;citation&quot; data-citation=&quot;%7B%22citationItems%22%3A%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%225%22%7D%5D%2C%22properties%22%3A%7B%7D%7D&quot;&gt;(&lt;span class=&quot;citation-item&quot;&gt;Karlsson Holst och Nilsson, 2022, p. 5&lt;/span&gt;)&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;span class=&quot;highlight&quot; data-annotation=&quot;%7B%22attachmentURI%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FPI6P72HR%22%2C%22annotationKey%22%3A%22VCMWF2TT%22%2C%22color%22%3A%22%23e56eee%22%2C%22pageLabel%22%3A%225%22%2C%22position%22%3A%7B%22pageIndex%22%3A4%2C%22rects%22%3A%5B%5B263.631%2C519.121%2C523.868%2C529.66%5D%2C%5B71.053%2C503.281%2C494.92%2C513.82%5D%2C%5B71.053%2C487.441%2C495.203%2C497.98%5D%2C%5B71.053%2C471.601%2C102.049%2C482.14%5D%5D%7D%2C%22citationItem%22%3A%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%225%22%7D%7D&quot;&gt;&lt;span style=&quot;background-color: #e56eee80&quot;&gt;“Om denna grund är svag saknar eleven förutsättningar att bygga upp en grundläggande förståelse för ämnet matematik och det finns risk för att eleven utvecklar matematiksvårigheter under senare skolår (Aubrey et al., 2006; Griffin, 2016).”&lt;/span&gt;&lt;/span&gt; &lt;span class=&quot;citation&quot; data-citation=&quot;%7B%22citationItems%22%3A%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%225%22%7D%5D%2C%22properties%22%3A%7B%7D%7D&quot;&gt;(&lt;span class=&quot;citation-item&quot;&gt;Karlsson Holst och Nilsson, 2022, p. 5&lt;/span&gt;)&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;span class=&quot;highlight&quot; data-annotation=&quot;%7B%22attachmentURI%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FPI6P72HR%22%2C%22annotationKey%22%3A%22GHTFSFRR%22%2C%22color%22%3A%22%23f19837%22%2C%22pageLabel%22%3A%225%22%2C%22position%22%3A%7B%22pageIndex%22%3A4%2C%22rects%22%3A%5B%5B359.92%2C392.161%2C505.214%2C402.7%5D%2C%5B71.053%2C376.321%2C473.947%2C386.861%5D%2C%5B71.053%2C360.481%2C510.958%2C371.02%5D%5D%7D%2C%22citationItem%22%3A%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%225%22%7D%7D&quot;&gt;&lt;span style=&quot;background-color: #f1983780&quot;&gt;“Att organisera undervisningen menas i denna studie hur skolor ger förutsättningar för undervisningen i form av tex personalresurser, möjligheter till uppdelning i mindre grupper, specialpedagogiskt stöd osv.”&lt;/span&gt;&lt;/span&gt; &lt;span class=&quot;citation&quot; data-citation=&quot;%7B%22citationItems%22%3A%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%225%22%7D%5D%2C%22properties%22%3A%7B%7D%7D&quot;&gt;(&lt;span class=&quot;citation-item&quot;&gt;Karlsson Holst och Nilsson, 2022, p. 5&lt;/span&gt;)&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;span class=&quot;highlight&quot; data-annotation=&quot;%7B%22attachmentURI%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FPI6P72HR%22%2C%22annotationKey%22%3A%22DIIHFSBZ%22%2C%22color%22%3A%22%23aaaaaa%22%2C%22pageLabel%22%3A%225%22%2C%22position%22%3A%7B%22pageIndex%22%3A4%2C%22rects%22%3A%5B%5B71.053%2C288.961%2C453.956%2C299.5%5D%2C%5B71.053%2C273.121%2C487.215%2C283.661%5D%5D%7D%2C%22citationItem%22%3A%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%225%22%7D%7D&quot;&gt;&lt;span style=&quot;background-color: #aaaaaa80&quot;&gt;“En förutsättning för att kunna sätta in tidigt stöd för att förebygga och motverka matematiksvårigheter är att i ett tidigt skede upptäcka tecken på matematiksvårigheter.”&lt;/span&gt;&lt;/span&gt; &lt;span class=&quot;citation&quot; data-citation=&quot;%7B%22citationItems%22%3A%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F7417009%2Fitems%2FY8RW2TIW%22%5D%2C%22locator%22%3A%225%22%7D%5D%2C%22properties%22%3A%7B%7D%7D&quot;&gt;(&lt;span class=&quot;citation-item&quot;&gt;Karlsson Holst och Nilsson, 2022, p. 5&lt;/span&gt;)&lt;/span&gt;&lt;/p&gt;
&lt;/div&gt;</rdf:value>
